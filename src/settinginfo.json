        "type": "taglist", "parser": "save_tags"
    },
    "tag_chair": {
        "name": "Chair-only tags",
        "type": "taglist", "parser": "save_tags"
    },
    "tag_color": {
        "name": "Tag colors",
        "type": "taglist", "parser": "save_tags"
    },
    "tag_rank": {
        "name": "Ranking tags",
        "type": "taglist", "parser": "save_tags"
    },
    "tag_seeall": {
        "name": "PC can see tags for conflicted papers",
        "type": "checkbox"
    },
    "tag_vote": {
        "name": "Voting tags",
        "type": "taglist", "parser": "save_tags"
    },
    "topics": {
