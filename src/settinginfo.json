    },
    "rev_blind": {
        "name": "Review anonymity",
        "type": "radio", "values": [0, 1, 2]
    },
    "rev_notifychair": {
        "name": "Notify PC chairs of new reviews by email",
        "type": "checkbox"
    },
    "rev_open": {
        "name": "Open site for reviewing",
        "type": "cdate"
    },
    "rev_ratings": {
        "name": "Collect ratings of reviews",
        "type": "radio", "values": [0, 1, 2]
    },
    "rev_roundtag": {
        "name": "Current review round",
        "type": "tag", "parser": "save_rounds"
    },
    "reviewform": {
        "parser": "save_review_form", "require": "src/settings/s_reviewform.php"
    },
    "seedec": {
        "name": "Decision visibility",
        "type": "radio", "values": [0, 1, 2, 3]
    },
    "sub_banal": {
        "type": "checkbox", "parser": "save_banal"
    },
    "sub_blind": {
        "name": "Submission anonymity",
        "type": "radio", "values": [0, 1, 2, 3]
    },
    "sub_collab": {
        "name": "Collect other collaborators",
        "type": "checkbox"
    },
    "sub_freeze": {
